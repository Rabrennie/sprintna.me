{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview --port 8080 --host 0.0.0.0",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "STORYBOOK=true storybook dev -p 6006",
    "build-storybook": "STORYBOOK=true storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0-beta.43",
    "@storybook/addon-interactions": "^7.0.0-beta.43",
    "@storybook/addon-links": "^7.0.0-beta.43",
    "@storybook/addon-svelte-csf": "^3.0.0-next.3",
    "@storybook/blocks": "^7.0.0-beta.43",
    "@storybook/svelte": "^7.0.0-beta.43",
    "@storybook/sveltekit": "^7.0.0-beta.43",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-node": "^1.1.7",
    "@sveltejs/kit": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.50.0",
    "esbuild": "^0.17.6",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "express": "^4.18.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prisma": "^4.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.5.4",
    "storybook": "^7.0.0-beta.43",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.10.0",
    "@types/express": "^4.17.17",
    "animejs": "^3.2.1",
    "socket.io": "^4.6.0"
  }
}
